node{
    stage('Checkout SCM'){
        git branch: 'Origin', url:'https://github.com/nosby2007/JADE-SHOP.git'
        
    }
    stage('install node modules'){
        sh "npm install --legacy-peer-deps"

    }
    stage('Build'){
        sh "npm run build:ssr"

    }
    stage('Deploy'){
        sh "pm2 restart all"
    }
}
